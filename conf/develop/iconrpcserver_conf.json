{
    "log": {
        "logger": "iconrpcserver",
        "colorLog": true,
        "level": "warning",
        "filePath": "./log/develop/iconrpcserver.log",
        "outputType": "console|file",
        "rotate": {
            "type": "period|bytes",
            "period": "daily",
            "interval": 1,
            "backupCount": 10,
            "maxBytes": 50000000
        }
    },
    "gunicornConfig": {
        "workers": 1,
        "graceful_timeout": 0
    }
}
